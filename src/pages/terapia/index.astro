---
import Layout from "../../layouts/layout.astro";
import { fisioContent } from "../../content/fisio.ts";

import HeroSection from "../../components/HeroSection.astro";
import Hero1 from "../../assets/fisio/FisioHero2.jpg";
import Hero2 from "../../assets/fisio/FisioHero1.jpg";

import ServicesSection from "../../components/ServicesSection.astro";
import FisioLogo from "../../assets/fisio/Fisio.svg";

import AboutUsSection from "../../components/AboutUsSection.astro";
import AboutUsImage from "../../assets/dental/AboutUsDentist.jpg";

import TestimonialsSection from "../../components/TestimonialsSection.astro";

import ContactSection from "../../components/ContactSection.astro";
import LogoFisio from "../../assets/fisio/LogoFisio.svg";

import Footer from "../../components/Footer.astro";
---

<Layout>
  <HeroSection {...fisioContent.hero} hero1={Hero1} hero2={Hero2} />
  <ServicesSection services={fisioContent.services} ServiceLogo={FisioLogo} />
  <AboutUsSection
    {...fisioContent.aboutUs}
    AboutUsImg={AboutUsImage}
    Doctors={fisioContent.aboutUs.doctors}
  />
  <TestimonialsSection testimonials={fisioContent.testimonials} />
  <ContactSection {...fisioContent.contact} />
  <Footer
    name={fisioContent.hero.name}
    email={fisioContent.contact.email}
    LogoFooter={LogoFisio}
  />
</Layout>
